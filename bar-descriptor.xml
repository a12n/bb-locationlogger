<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>org.bitbucket.a12n.bb.gnsslogger</id>
    <configuration name="Device-Debug">
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm/o.le-v7-g/gnsslogger" entry="true" type="Qnx/Elf">gnsslogger</asset>
    </configuration>
    <configuration name="Device-Release">
       <entryPointType>Qnx/Cascades</entryPointType>
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm/o.le-v7/gnsslogger.so" entry="true" type="Qnx/Elf">gnsslogger.so</asset>
    </configuration>
    <configuration name="Device-Profile">
       <platformArchitecture>armle-v7</platformArchitecture>
       <asset path="arm-p/o.le-v7-g/gnsslogger" entry="true" type="Qnx/Elf">gnsslogger</asset>
    </configuration>
    <configuration name="Simulator-Debug">
       <platformArchitecture>x86</platformArchitecture>
       <asset path="x86/o-g/gnsslogger" entry="true" type="Qnx/Elf">gnsslogger</asset>
    </configuration>
    <name>
       GNSS Logger
       <text xml:lang="ru">GNSS &#x440;&#x435;&#x433;&#x438;&#x441;&#x442;&#x440;&#x430;&#x442;&#x43e;&#x440;</text>
    </name>
    <versionNumber>0.7.1</versionNumber>
    <buildId>1</buildId>
    <packageLocale>ru</packageLocale>
    <!-- <versionLabel></versionLabel> -->
    <description>
       Logs device coordinates and altitude to a GPX file.
       <text xml:lang="ru">&#x417;&#x430;&#x43f;&#x438;&#x441;&#x44b;&#x432;&#x430;&#x435;&#x442; &#x43a;&#x43e;&#x43e;&#x440;&#x434;&#x438;&#x43d;&#x430;&#x442;&#x44b; &#x443;&#x441;&#x442;&#x440;&#x43e;&#x439;&#x441;&#x442;&#x430; &#x432; GPX &#x444;&#x430;&#x439;&#x43b;.</text>
    </description>
    <!-- <copyright></copyright> -->
    <author>a12n</author>
    <!-- <authorId>ABC1234YjsnUk235h</authorId> -->

    <initialWindow>
        <autoOrients>true</autoOrients>
        <systemChrome>none</systemChrome>
    </initialWindow>

    <icon>
        <image>icon.png</image>
    </icon>

    <asset path="icon.png">icon.png</asset>
    <asset path="assets">assets</asset>
    <asset path="translations" dest="qm">
        <include name="*.qm"/>
    </asset>

    <permission system="true">run_native</permission>
    <permission>access_location_services</permission>
    <permission>access_shared</permission>
    <permission>run_when_backgrounded</permission>

    <env var="LD_LIBRARY_PATH" value="app/native/lib:/usr/lib/qt4/lib"/>
</qnx>
